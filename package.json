{
  "name": "@ionic/cloud-angular",
  "version": "0.12.0",
  "description": "Ionic Cloud Client for Angular 2",
  "main": "dist/es5/index.js",
  "module": "dist/esm/index.js",
  "typings": "dist/es5/index.d.ts",
  "scripts": {
    "lint": "tslint 'src/**/*.ts'",
    "tsc": "tsc -p tsconfig-esm.json",
    "ngc": "ngc -p tsconfig-ngc.json",
    "build": "npm run lint && npm run ngc && npm run tsc",
    "prepublish": "npm run build",
    "watch": "./scripts/watch.sh"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/driftyco/ionic-cloud-angular.git"
  },
  "keywords": [
    "Ionic",
    "ionic",
    "cloud",
    "web",
    "client",
    "angular"
  ],
  "author": "Ionic",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/driftyco/ionic-cloud-angular/issues"
  },
  "homepage": "https://github.com/driftyco/ionic-cloud-angular#readme",
  "dependencies": {
    "@ionic/cloud": "0.16.0"
  },
  "peerDependencies": {
    "@angular/core": "^2.1.1 || ^4.0.0",
    "rxjs": "^5.0.1"
  },
  "devDependencies": {
    "@angular/compiler": "^2.1.1",
    "@angular/compiler-cli": "^2.1.1",
    "chokidar-cli": "tlancina/chokidar-cli",
    "tslint": "^3.10.2",
    "tslint-ionic-rules": "*",
    "typescript": "^2.2.0"
  }
}
